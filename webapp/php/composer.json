{
    "name": "isucon9-qualify/isucari",
    "description": "A sample app for Isucon9",
    "keywords": ["isucon", "isucon9"],
    "homepage": "http://isucon.net/",
    "license": "MIT",
    "require": {
        "php": "^8.4 || ^8.5",
        "ext-pdo": "*",
        "bryanjhv/slim-session": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^3.0",
        "slim/php-view": "^3.0",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8000 -t public public/index.php"
    }
}
